<h1>mainpage</h1>
<% @plans.each do |plan| %>
  <div>
  <%= link_to plan.title, "/plans/#{plan.id}", method: :get %>
  </div>
  <div>
  <%= plan.user.name %>
  </div>
  <div>
  <div>
  <%= plan.region %>
  </div>
  <div>
  <%= plan.date %>
  </div>
  <div>
  <%= plan.image %>
  </div>
  <div>
  <%= plan.details %>
  </div>
  <br>
<% end %>
